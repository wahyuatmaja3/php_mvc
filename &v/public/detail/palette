$middle-grey: #8a817cff;
$pale-silver: #bcb8b1ff;
$isabelline: #f4f3eeff;
$melon: #e0afa0ff;

/* SCSS HSL */
$dark-lava: hsla(25, 9%, 25%, 1);
$middle-grey: hsla(21, 6%, 51%, 1);
$pale-silver: hsla(38, 8%, 72%, 1);
$isabelline: hsla(50, 21%, 95%, 1);
$melon: hsla(14, 51%, 75%, 1);

/* SCSS RGB */

$middle-grey: rgba(138, 129, 124, 1);
$pale-silver: rgba(188, 184, 177, 1);
$isabelline: rgba(244, 243, 238, 1);
$melon: rgba(224, 175, 160, 1);

/* SCSS Gradient */
$gradient-top: linear-gradient(0deg, #463f3aff, #8a817cff, #bcb8b1ff, #f4f3eeff, #e0afa0ff);
